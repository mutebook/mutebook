{
  "version": "0.2.0",
  "configurations": [
  {
    "type": "chrome",
    "request": "launch",
    "name": "Chrome",
    "url": "http://localhost:8053",
    "webRoot": "${workspaceRoot}"
  },
    {
      "name": "XDebug",
      "type": "php",
      "request": "launch",
      "port": 9000
    },
    {
      "name": "php file",
      "type": "php",
      "request": "launch",
      "program": "${file}",
      "cwd": "${fileDirname}",
      "port": 9000
    }
  ]
}